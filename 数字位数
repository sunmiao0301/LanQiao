import java.util.*;
// 1:无需package
// 2: 类名必须Main, 不可修改

public class Main {
    public static void main(String[] args) {
        Scanner scan = new Scanner(System.in);
        //在此输入您的代码...
        int ret = 0;
        for(int i = 1; i < 2021; i++){
          String s = Integer.toString(i);
          ret += s.length();
        }
        System.out.print(ret);
        scan.close();
    }
}
