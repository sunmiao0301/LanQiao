import java.util.Scanner;
// 1:无需package
// 2: 类名必须Main, 不可修改

public class Main {
    public static void main(String[] args) {
        Scanner scan = new Scanner(System.in);
        //在此输入您的代码...
        double ret = 1250 + (30000 - 1250 * 14) * 0.005;
        //注意是"%.2f" 不要遗漏引号
        System.out.printf("%.2f", ret);
        scan.close();
    }
}
