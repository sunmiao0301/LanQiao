import java.util.Scanner;
// 1:无需package
// 2: 类名必须Main, 不可修改

import java.math.*;

public class Main {
    public static void main(String[] args) {
        Scanner scan = new Scanner(System.in);
        //在此输入您的代码...
        int ret = 0;
        for(int i = 0; i < 1000; i++){
          if(Math.pow(2, i) < 10000 && 10000 < Math.pow(2, i+1)){
            ret = i + 1;
          }
        }
        System.out.print(ret);
        scan.close();
    }
}
